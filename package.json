{
  "name": "NNBox",
  "displayName": "NNBox",
  "description": "Extension for il files generated by nncase.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.80.0"
  },
  "keywords": [
    "nncase"
  ],
  "publisher": "FusionBolt",
  "files.associations": {
    "*.il": "nil"
  },
  "categories": [
    "Other"
  ],
  "icon": "icons/logo.png",
  "activationEvents": [
    "workspaceContains:**/*.il",
    "onLanguage:nil"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/FusionBolt/NNBox.git"
  },
  "license": "MIT",
  "contributes": {
    "languages": [{
        "id": "nil",
        "aliases": ["nil"],
        "extensions": ["*.il"],
        "configuration": "./language-configuration.json"
      }],
      "grammars": [{
        "language": "nil",
        "scopeName": "source.il",
        "path": "./syntaxes/il.tmLanguage.json"
      }],
      "commands": [
        {
          "command": "goto.nnbox",
          "title": "goto nncase test",
          "icon": "$(go-to-file)"
        },
        {
          "command": "runTest.nnbox",
          "title": "run nncase test",
          "icon": "$(run)"
        },
        {
          "command": "github.nncase.nnbox",
          "title": "goto nncase github page",
          "icon": "$(github)"
        }
      ],
      "menus": {
        "editor/title": [
          {
            "command": "goto.nnbox",
            "group": "navigation",
            "when": "editorLangId == nil"
          },
          {
            "command": "runTest.nnbox",
            "group": "navigation",
            "when": "editorLangId == nil"
          },
          {
            "command": "github.nncase.nnbox",
            "group": "navigation",
            "when": "editorLangId == nil"
          }
        ]
      },
      "configuration":[
        {
          "title": "NNBox",
          "properties": {
            "gotoCommand": {
              "type": "string",
              "default": "",
              "description": "Command used for goto test source code, default use vscode\nyou can set other texteditor like command \"rider --line $0 $1\", $0 is line number, $1 is path"
            },
            "nncaseRootDir": {
              "type": "string",
              "default": "",
              "description": "Nncase src root dir, if it is not set then it will be searched automatically"
            },
            "kpuRootDir": {
              "type": "string",
              "default": "",
              "description": "KPU src root dir, if it is not set then it will be searched automatically"
            }
          }
        }
      ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build:syntax": "ts-node src/typescript/GenerateTmLanguageFile.ts > ./syntaxes/il.tmLanguage.json",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "dependencies": {
    "@types/vscode": "^1.80.0",
    "vscode-languageclient": "^8.1.0",
    "vscode-languageserver": "^7.0.0",
    "vscode-languageserver-textdocument": "^1.0.1"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
